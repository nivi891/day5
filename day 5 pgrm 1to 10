#include<stdio.h>
int main()
{
int n;
scanf("%d",&n);
if(n>0 && (n&(n-1))==0)
printf("yes");
else
printf("no");
return 0;
}





#include <stdio.h>

int main() {
    int start, end;
    scanf("%d %d", &start, &end);  

    int i = start;
    while(i <= end) {
        if(i % 2 == 0) {  
            printf("%d ", i); 
        }
        i++;  
    }

    return 0;
}





#include <stdio.h>

int main()
{
    int end;
    scanf("%d", &end);  
    int i = 1;
    while(i <= end)
    {
        printf("%d ", i);
        i++;
    }
    return 0;
}





#include <stdio.h>

int main()
{
int a,b,c=0;
scanf("%d %d",&a,&b);
while(a>=b)
{
 a=a-b;
 c++;
}
printf("%d",c);

}


#include<stdio.h>

int main()
{
int a,b;

scanf("%d %d",&a,&b);

int m=(a>=b)?a:b;

while(1)

{

if(m%a==0 && m%b==0)

{

printf("%d",m);

break;

}

m++;




}


}



#include <stdio.h>

int main()
{

int a, sum=0;

scanf("%d",&a);

int i=1;

while(i<=a)

{

if(i%2==0)

{

sum=sum+i;

}

i++;
}

printf("%d", sum);
}



#include <stdio.h>

int main()
{
    int a,b,c=0,i;
    scanf("%d %d",&a,&b);//5 2
    for(i=0;i<b;i++)
    {
        c=c+a;//10
    }
    printf("%d ",c);//10

    return 0;
}



#include <stdio.h>

int main(){

int a,i;

scanf("%d",&a);

for(i=1;i<=a; i++)

{

printf("abcdefghijklmnopqrstuvwxyz");

}

return 0;

}





#include <stdio.h>

int main()
{
    int end,i;
    scanf("%d", &end);
    for(i=1;i<=end;i++)
    {
        if(i%2!=0)
{
            printf("%d",i);
           
}
    }
}



#include <stdio.h>

int main() {
    int multiplicand, n;
    scanf("%d %d", &multiplicand, &n);


    for (int i = 1; i <= multiplicand; i++) {
        printf("%d * %d = %d\n", i, n, i * n);
    }

    return 0;
}






