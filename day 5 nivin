#include<stdio.h>
int main()
{
int n;
scanf("%d",&n);
if(n>0 && (n&(n-1))==0)
printf("yes");
else
printf("no");
return 0;
}





#include <stdio.h>

int main() {
    int start, end;
    scanf("%d %d", &start, &end);  

    int i = start;
    while(i <= end) {
        if(i % 2 == 0) {  
            printf("%d ", i); 
        }
        i++;  
    }

    return 0;
}





#include <stdio.h>

int main()
{
    int end;
    scanf("%d", &end);  
    int i = 1;
    while(i <= end)
    {
        printf("%d ", i);
        i++;
    }
    return 0;
}
